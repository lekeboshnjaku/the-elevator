    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.21",
    "esbuild": "^0.23.0",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "serve": "^14.2.4",
    "tailwindcss": "^4.1.12",
    "ts-loader": "^9.5.4",
    "typescript": "^5.5.3",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1"
