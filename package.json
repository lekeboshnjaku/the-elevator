  "description": "A casino-style Limbo game with an elevator theme. Players bet on a target multiplier for a chance to win. This is a simulation and does not involve real money.",
  "main": "index.js",
  "scripts": {
    "start": "serve front",
    "build:css": "npx @tailwindcss/cli -i ./tailwind.input.css -o ./styles.tailwind.css --minify",
    "build": "webpack --config webpack.config.js",
    "typecheck": "tsc --noEmit",
    "type-check": "tsc --noEmit",
    "build:math": "math_sdk_project\\env\\Scripts\\python.exe math_sdk_project\\games\\limbo_game\\run.py"
  },
  "dependencies": {
    "@fontsource/anton": "^5.2.6",
    "@fontsource/chakra-petch": "^5.2.6",
    "@fontsource/orbitron": "^5.2.6",
